{
  "name": "@recharts/devtools",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "version": "0.0.8",
  "description": "Devtools and helper functions for Recharts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/recharts/devtools"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src test",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "release": "node ./scripts/publish.js"
  },
  "keywords": [
    "recharts",
    "devtools",
    "charts"
  ],
  "author": "Recharts Group",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^15.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^1.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "latest"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "recharts": "^3.3.0"
  }
}
